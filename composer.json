{
    "name": "jfnetwork/doctrine-encrypted-object",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Doctrine mapping type to store encrypted objects",
    "require": {
        "php": ">=8.1",
        "doctrine/orm": "^2.5",
        "symfony/console": "5.*|6.*|7.*",
        "symfony/config": "5.*|6.*|7.*",
        "symfony/http-kernel": "5.*|6.*|7.*",
        "symfony/service-contracts": "*",
        "symfony/dependency-injection": "5.*|6.*|7.*"
    },
    "suggest": {
        "ext-openssl": "*",
        "ext-sodium": "*",
        "defuse/php-encryption": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Jfnetwork\\DoctrineEncryptedObject\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Anton Eremin",
            "homepage": "http://jfnet.de/"
        }
    ]
}
